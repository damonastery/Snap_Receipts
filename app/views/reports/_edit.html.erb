<div id="edit_report_<%= report.id %>" class="form-box">
  <%= simple_form_for report, remote: true do |f| %>
    <div class="form-group">
      <%= f.input :title, class: "form-control" %>
    </div>
    <div class="form-group">
      <%= f.input :description, class: "form-control" %>
    </div>
    <%= f.submit class: "btn btn-success" %>
  <% end %>
</div>

<script>
  reportEditor.initialize(<%= report.id %>)
</script>