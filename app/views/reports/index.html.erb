<h1>Reports#index</h1>
<p>Find me in app/views/reports/index.html.erb</p>

<%= link_to "NEW", new_report_path %>
<hr>

<% @reports.each do |report| %>
  <%= report.title %>
  <%= report.description %>
  <%= link_to "edit", edit_report_path(report) %>
  <%= link_to "Delete", report_path(report), method: :delete %>
  <hr>
<% end %>
